{
  "name": "g-wrapper",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "sw": "npm run sw-dev && npm run sw-prod",
    "test:watch": "vitest watch",
    "sw-dev": "esbuild ./serviceWorkers/dev.ts --bundle --minify --outfile=./public/dev-sw.js",
    "sw-prod": "esbuild ./serviceWorkers/prod.ts --bundle --minify --outfile=./public/sw.js"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "esbuild": "^0.15.17",
    "nanoid": "^4.0.0",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.5"
  }
}
